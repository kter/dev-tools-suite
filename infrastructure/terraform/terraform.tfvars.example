# Example Terraform variables file
# Copy this to terraform.tfvars and customize

# Google Cloud Project ID
project_id = "dev-tools-suite"

# Google Cloud Region
region = "us-central1"

# Google Cloud Zone  
zone = "us-central1-a"

# Environment (dev or prd)
environment = "dev"

# Domain suffix
domain_suffix = "dev.devtools.site"