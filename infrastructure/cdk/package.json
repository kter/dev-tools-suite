{
  "name": "dev-tools-cdk",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "diff:dev": "cdk diff --profile dev --context environment=dev --all",
    "diff:prd": "cdk diff --profile prd --context environment=prd --all",
    "deploy:dev": "cdk deploy --profile dev --context environment=dev --all",
    "deploy:prd": "cdk deploy --profile prd --context environment=prd --all",
    "destroy:dev": "cdk destroy --profile dev --context environment=dev --all",
    "destroy:prd": "cdk destroy --profile prd --context environment=prd --all"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/node": "20.1.0",
    "jest": "^29.4.0",
    "ts-jest": "^29.0.5",
    "typescript": "~5.0.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0"
  }
}